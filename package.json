{
  "name": "postgraphile-ts",
  "version": "1.0.0",
  "description": "Projeto de API GraphQL utilizando a combinação do Typescript, Postgraphile e PostgreSQL.",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "build:clean": "tsc --build --clean",
    "build:watch": "tsc --watch --sourceMap",
    "rm:dist": "node scripts/clean.js",
    "lint": "eslint src/",
    "build:dev": "run-s lint build:clean rm:dist build:watch",
    "server:start": "nodemon scripts/server.js",
    "start": "run-p build:dev server:start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglaspands/postgraphile-ts.git"
  },
  "keywords": [
    "typescript",
    "postgraphile"
  ],
  "author": "@douglaspands",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/douglaspands/postgraphile-ts/issues"
  },
  "homepage": "https://github.com/douglaspands/postgraphile-ts#readme",
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "postgraphile": "^4.13.0"
  }
}
