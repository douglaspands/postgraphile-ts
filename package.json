{
  "name": "postgraphile-ts",
  "version": "1.0.0",
  "description": "Projeto de API GraphQL utilizando a combinação do Typescript, Postgraphile e PostgreSQL.",
  "scripts": {
    "tsc": "tsc",
    "tsc:alias": "tsc-alias",
    "tsc:alias:watch": "tsc-alias --watch",
    "tsc:clean": "tsc --build --clean",
    "tsc:watch": "tsc --watch --sourceMap",
    "build": "run-s rm:dist lint tsc tsc:alias",
    "build:watch": "npm-run-all build --parallel tsc:watch tsc:alias:watch",
    "rm:dist": "node scripts/clean.js",
    "lint": "eslint ./src",
    "start:ts": "nodemon ./scripts/server.ts",
    "start:js": "node ./scripts/server.js",
    "start": "run-s start:ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglaspands/postgraphile-ts.git"
  },
  "keywords": [
    "typescript",
    "postgraphile"
  ],
  "author": "@douglaspands",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/douglaspands/postgraphile-ts/issues"
  },
  "homepage": "https://github.com/douglaspands/postgraphile-ts#readme",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "postgraphile": "^4.13.0",
    "winston": "^3.8.2"
  }
}
